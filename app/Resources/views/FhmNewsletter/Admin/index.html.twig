{% trans_default_domain instance.domain %}
{% extends "::FhmFhm/Admin/index.html.twig" %}

{% block content_data_header_language %}{% endblock %}
{% block content_data_language %}{% endblock %}

{% block content_data_header %}
    <th>{{ (instance.translation ~ '.admin.index.table.header.email')|trans }}</th>
    <th>{{ (instance.translation ~ '.admin.index.table.header.date')|trans }}</th>
{% endblock %}

{% block content_data_body %}
    <td>{{ document.getEmail() }}</td>
    <td>{{ document.getDateCreate() | localizeddate('full', 'short', app.request.locale) }}</td>
{% endblock %}