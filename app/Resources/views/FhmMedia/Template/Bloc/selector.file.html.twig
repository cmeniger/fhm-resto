{% trans_default_domain instance.domain %}

<div class='data clickable file {{ object.getExtension() }} {{ object.getId() == selected ? 'selected' : '' }}' media-id='{{ object.getId() }}' media-name='{{ object.getName() }}' media-extension='{{ object.getExtension() }}' media-type='file' title='{{ object.getName() }}'>
    <div class="name">
        {{ object.getName() }}
    </div>
    <div class="extension right">
        <span class="radius label secondary">{{ object.getExtension() }}</span>
    </div>
</div>