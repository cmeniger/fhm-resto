{% trans_default_domain instance.domain %}
{% extends "::FhmMapPicker/Map/template.html.twig" %}

{% block map_region_over %}
    if(typeof ze{{ document.getId() }}[code] == 'undefined')
    {
        event.preventDefault();
    }
{% endblock %}

{% block map_region_click %}
    if(typeof ze{{ document.getId() }}[code] == 'undefined')
    {
        event.preventDefault();
    }
    else
    {
        window.location.href = ze{{ document.getId() }}[code];
    }
{% endblock %}